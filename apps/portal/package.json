{
  "name": "local-plans-portal",
  "version": "0.0.0",
  "private": true,
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "npm run build && nodemon --watch src --ext js,ts,scss,html,njk --exec \"npm run start\"",
    "build": "node src/util/build.ts",
    "start": "node src/server.ts",
    "test": "node --test src/**/*.test.ts",
    "test:watch": "node --test --watch src/**/*.test.ts",
    "test:coverage": "node --test --experimental-test-coverage src/**/*.test.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@pins/local-plans-database": "^0.0.0",
    "@pins/local-plans-lib": "^0.0.0",
    "@pins/local-plans-questionnaire": "^0.0.0",
    "@planning-inspectorate/dynamic-forms": "*",
    "body-parser": "*",
    "connect-redis": "*",
    "dotenv": "*",
    "express": "*",
    "express-session": "*",
    "express-validator": "*",
    "govuk-frontend": "*",
    "helmet": "*",
    "nunjucks": "*",
    "pino": "*",
    "pino-pretty": "*",
    "redis": "*"
  },
  "devDependencies": {
    "cypress": "^15.8.1",
    "nodemon": "*",
    "sass": "*"
  },
  "imports": {
    "#service": "./src/app/service.ts",
    "#util/*": "./src/util/*"
  }
}
